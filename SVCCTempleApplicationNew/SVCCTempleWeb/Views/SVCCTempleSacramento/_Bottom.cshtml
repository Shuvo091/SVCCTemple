<a href="@ApplicationInstance.Application["SacramentoTempleWebUrl2"].ToString()" style="color:#0000ff; font-family: Arial; font-size: 9pt; text-decoration: underline;" target="_blank">@ApplicationInstance.Application["SacramentoTempleWebUrl2"].ToString()</a>&nbsp;/&nbsp;
