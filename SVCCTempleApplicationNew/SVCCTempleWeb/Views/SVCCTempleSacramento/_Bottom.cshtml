<div class="fb-like" data-href="@ApplicationInstance.Application["SacramentoTempleFacebookUrl"].ToString()" data-send="false" data-layout="button_count" data-width="100" data-show-faces="false" style="vertical-align:top;zoom:1;display:inline"></div>
<a href="@ApplicationInstance.Application["SacramentoTempleFacebookUrl"].ToString()" target="_blank"><img src="@ApplicationInstance.Application["SacramentoImagesBaseURL"]/Facebook.jpg" alt="Find us on Facebook (@ApplicationInstance.Application["SacramentoTempleName"].ToString())" title="Find us on Facebook (@ApplicationInstance.Application["SacramentoTempleName"].ToString())" /></a>
<div id="divFacebook" style="margin-left:auto;margin-right:auto;width:240px"></div>
<script type="text/javascript">
    (
        function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/en_US/all.js#xfbml=1";
            fjs.parentNode.insertBefore(js, fjs);
        }
            (document, 'script', 'facebook-jssdk')
    );
</script>
