@model SVCCTempleWeb.Models.SankalpamInfoModel

<div class="col-md-9">
    <div scope="col" style="align-content: center; background-color: #ffffff; color: #000000; display: flex; flex-direction: column; font-size: 18px; font-weight: bold; justify-content: center; text-align: center;">
        <h4 style="font-weight: bold;">Sankalpam for @Model.StartDate.ToString("ddd MMM dd yyyy") - @Model.FinishDate.ToString("ddd MMM dd yyyy")</h4>
    </div>
</div>
@foreach (var sankalpamInfoDetailModel in Model.SankalpamInfoDetailModels)
{
    @Html.Partial("_SankalpamData", sankalpamInfoDetailModel)
}
<br style="clear: both;" />
@Html.Partial("_RituSeason")
