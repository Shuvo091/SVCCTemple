@model SVCCTempleWeb.Models.IndexModel
@using ArchitectureLibraryUtility
@{
    int i1, j1, k1, count;
    string beginDate1;
}
<div class="col-md-9">
    <table style="border-collapse: collapse; border-color: #000000; border-style: solid; border-width: 1px; font-size: 12px; padding: 5px;">
        @{
            for (i1 = 0; i1 < Model.TempleEventImportantDatesModels.Count; i1++)
            {
                <tr>
                    <th scope="row" style="border-collapse: collapse; border-color: #000000; border-style: solid; border-width: 1px; padding: 5px; vertical-align: top;" title="@Model.TempleEventImportantDatesModels[i1].EventText1 @Model.TempleEventImportantDatesModels[i1].EventText2 @Model.TempleEventImportantDatesModels[i1].EventText3">
                        <img src="@(ApplicationInstance.Application["BaseURL"] + "Images/Fremont/ImportantDates/" + Model.TempleEventImportantDatesModels[i1].ImageFileName1)" style="width: 145px;" />
                    </th>
                    <th scope="row" style="border-collapse: collapse; border-color: #000000; border-style: solid; border-width: 1px; padding: 5px; vertical-align: top; width: 153px;" title="@Model.TempleEventImportantDatesModels[i1].EventText1 @Model.TempleEventImportantDatesModels[i1].EventText2 @Model.TempleEventImportantDatesModels[i1].EventText3">
                        @Model.TempleEventImportantDatesModels[i1].EventText1<br />
                        <span style="color: #ff0000;">@Model.TempleEventImportantDatesModels[i1].EventText2</span>
                        <br />
                        <span style="color: #0000ff;">@Model.TempleEventImportantDatesModels[i1].EventText3</span>
                        <br /><br />
                        <a href="@(ApplicationInstance.Application["BaseURL"] + "Files/" + Model.TempleEventImportantDatesModels[i1].FileName1)" style="background-color: #d3d3d3; color: #000000; text-decoration: underline;" target="_blank">Read more...</a>
                        @if (Model.TempleEventImportantDatesModels[i1].SponsorshipGroupId != "")
                        {
                            <br />
                            <br />
                            <a href="@(ApplicationInstance.Application["FremontBaseURL"] + "Sponsorship/" + Model.TempleEventImportantDatesModels[i1].SponsorshipGroupId)" style="color: #0000ff; font-weight: bold; text-decoration: underline;" target="_blank">Sankalpam</a>
                        }
                    </th>
                    <td style="padding: 0px; vertical-align: top;" title="@Model.TempleEventImportantDatesModels[i1].EventText1 @Model.TempleEventImportantDatesModels[i1].EventText2 @Model.TempleEventImportantDatesModels[i1].EventText3">
                        <table style="border-collapse: collapse; border-color: #000000; border-style: solid; border-width: 1px; font-size: 12px; padding: 5px;">
                            @{
                                j1 = 0;
                                while (j1 < Model.TempleEventImportantDatesModels[i1].StartDates.Count)
                                {
                                    <tr>
                                        @{
                                            k1 = 0;
                                            while (j1 < Model.TempleEventImportantDatesModels[i1].StartDates.Count && k1 < 4)
                                            {
                                        <th scope="row" style="border-collapse: collapse; border-color: #000000; border-style: solid; border-width: 1px; font-size: 12px; padding: 5px; text-align: center; vertical-align: top; width: 135px;">
                                            @{
                                                beginDate1 = Model.TempleEventImportantDatesModels[i1].BeginDates[j1];
                                                count = 0;
                                                while (j1 < Model.TempleEventImportantDatesModels[i1].StartDates.Count && k1 < 4 && beginDate1 == Model.TempleEventImportantDatesModels[i1].BeginDates[j1])
                                                {
                                                    if (!string.IsNullOrWhiteSpace(Model.TempleEventImportantDatesModels[i1].StartDates[j1]))
                                                    {
                                                        if (count > 0)
                                                        {
                                                            <br /><br />
                                                        }
                                                        count++;
                                                        @DateTime.Parse(Model.TempleEventImportantDatesModels[i1].StartDates[j1]).ToString("ddd MMM-dd-yyyy")
                                                        if (!string.IsNullOrWhiteSpace(Model.TempleEventImportantDatesModels[i1].StartTimes[j1]) && Model.TempleEventImportantDatesModels[i1].StartTimes[j1] != "00:00")
                                                        {
                                                            <br />
                                                            <span style="color: #ff0000;">@DateTime.Parse(Model.TempleEventImportantDatesModels[i1].StartTimes[j1]).ToString("h:mm tt")</span>
                                                        }
                                                        if (long.Parse(Model.TempleEventImportantDatesModels[i1].TempleEventMasterId) == 51 || long.Parse(Model.TempleEventImportantDatesModels[i1].TempleEventMasterId) >= 60 && long.Parse(Model.TempleEventImportantDatesModels[i1].TempleEventMasterId) <= 63)
                                                        {
                                                            <br />
                                                            <span style="color: #0000ff;">@Model.TempleEventImportantDatesModels[i1].EventName1s[j1]</span>
                                                            <br />
                                                            <span style="color: #ff0000;">@Model.TempleEventImportantDatesModels[i1].EventName2s[j1]</span>
                                                        }
                                                        if (long.Parse(Model.TempleEventImportantDatesModels[i1].TempleEventMasterId) == 65)
                                                        {
                                                            <br />
                                                            <span style="color: #0000ff;">@Model.TempleEventImportantDatesModels[i1].EventName3s[j1]</span>
                                                        }
                                                    }
                                                    j1++;
                                                }
                                            }
                                        </th>
                                                k1++;
                                            }
                                        }
                                    </tr>
                                }
                            }
                        </table>
                    </td>
                    @{
                        @*j1 = 0;
                            while (j1 < Model.TempleEventImportantDatesModels[i1].StartDates.Count)
                            {
                                <th scope="row" style="border-collapse: collapse; border-color: #000000; border-style: solid; border-width: 1px; padding: 5px; text-align: center; vertical-align: top; width: 135px;">
                                    @{
                                        k1 = 0;
                                        while (j1 < Model.TempleEventImportantDatesModels[i1].StartDates.Count && k1 < 3)
                                        {
                                            beginDate1 = Model.TempleEventImportantDatesModels[i1].BeginDates[j1];
                                            while (j1 < Model.TempleEventImportantDatesModels[i1].StartDates.Count && k1 < 3 && beginDate1 == Model.TempleEventImportantDatesModels[i1].BeginDates[j1])
                                            {
                                                if (!string.IsNullOrWhiteSpace(Model.TempleEventImportantDatesModels[i1].StartDates[j1]))
                                                {
                                                    @DateTime.Parse(Model.TempleEventImportantDatesModels[i1].StartDates[j1]).ToString("ddd MMM-dd-yyyy")<br />
                                                    if (!string.IsNullOrWhiteSpace(Model.TempleEventImportantDatesModels[i1].StartTimes[j1]) && Model.TempleEventImportantDatesModels[i1].StartTimes[j1] != "00:00")
                                                    {
                                                        <span style="color: #ff0000;">@DateTime.Parse(Model.TempleEventImportantDatesModels[i1].StartTimes[j1]).ToString("h:mm tt")</span>
                                                        <br />
                                                    }
                                                    if (long.Parse(Model.TempleEventImportantDatesModels[i1].TempleEventMasterId) == 51 || long.Parse(Model.TempleEventImportantDatesModels[i1].TempleEventMasterId) >= 60 && long.Parse(Model.TempleEventImportantDatesModels[i1].TempleEventMasterId) <= 63)
                                                    {
                                                        <span style="color: #a55400;">@Model.TempleEventImportantDatesModels[i1].EventName1s[j1]</span>
                                                        <br />
                                                        <span style="color: #000000;">@Model.TempleEventImportantDatesModels[i1].EventName2s[j1]</span>
                                                        <br />
                                                    }
                                                }
                                                j1++;
                                            }
                                            k1++;
                                            <hr style="border-color: #000000; border-style: solid; border-width: 1px;" />
                                        }
                                    }
                                </th>
                            }*@
                    }
                </tr>
            }
        }
    </table>
</div>
