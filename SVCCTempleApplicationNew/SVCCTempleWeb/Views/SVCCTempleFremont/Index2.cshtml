<a href="@ApplicationInstance.Application["FremontBaseURL"]/KioskOrder" style="color: #0000ff; font-weight: bold; text-decoration: underline;" target="_blank"><span style="color: #ff0000;">Donate</span> through our <span style="color: #a54000; font-size: 23px;">KIOSK</span></a>
<span style="padding-right: 27px;">&nbsp;</span>
<a href="https://www.facebook.com/svcctemple" style="color: #0000ff; font-weight: bold; text-decoration: underline;" target="_blank">Click to watch live !!!</a>
<span style="padding-left: 27px;">&nbsp;</span>
<a href="@ApplicationInstance.Application["FremontBaseURL"]/Sponsorship" style="color: #ff0000; font-weight: bold; text-decoration: underline;" target="_blank"><span style="color: #0000ff;">Donate</span> & enter Sankalpam info</a>
<br style="clear: both;" />
@for (i = 0; i < tabTexts.Count; i++)
{
    <div style="background: #ddc385; border-top-left-radius: 25px; border-top-right-radius: 25px; float: left; height: 72px; margin-left: auto; margin-right: auto; padding: 8px; text-align: center; width: @tabWidths[i];">
        @if (i == Model.TabIndex)
        {
            <span style="color: #000000; font-family: Helvetica, Arial, Verdana, sans-serif; font-size: 15px; font-weight: bold;">@Html.Raw(tabTexts[i])</span>
        }
        else
        {
            <a id="divTab@(i)" href="@ApplicationInstance.Application["FremontBaseURL"]Index2/@(i)" style="color: #0000ff; font-family: Helvetica, Arial, Verdana, sans-serif; font-size: 15px; font-weight: bold; text-decoration: underline;">@Html.Raw(tabTexts[i])</a>
        }
    </div>
    if (i < tabTexts.Count - 1)
    {
        <div style="float: left; width: 9px;">&nbsp;</div>
    }
}
<br style="clear: both;" />
@switch (Model.TabIndex)
