{
"Today's Info" + "<br />" + Model.StartDate.ToString("ddd M / d") + "<br />" + "Sankalpam"
,"Important<br />Dates"
,"Festivals"
,"Varalakshmi<br />Vratham<br />Fri Aug 5"
,"Kids Ganesha<br />Puja<br />Aug 21"
};
    List<string> tabWidths = new List<string>
{
"117px"
,"135px"
,"99px"
,"117px"
,"135px"
};
    string imageTitle0 = "Varalakshmi Vratham - Aug 5 - Fri - SVCC Temple Fremont";
