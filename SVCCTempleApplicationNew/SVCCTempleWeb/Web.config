      <commandText value="INSERT ArchLib.ExceptionLog(LogDateTime, ApplicationName, IpAddress, ExecUniqueId, LoggedInUserId, CallingAssemblyName, ExecutingAssemblyName, CurrentMethodDeclaringType, MethodName, CallerLineNumber, ThreadNum, LogType, Logger, Message, DebugData, Exception) VALUES(@LogDateTime, @ApplicationName, @IPAddress, @ExecUniqueId, @LoggedInUserId, @CallingAssemblyName, @ExecutingAssemblyName, @CurrentMethodDeclaringType, @MethodName, @CallerLineNumber, @ThreadNum, @LogType, @Logger, @Message, @DebugData, @Exception)" />
